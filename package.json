{
  "name": "nativescript-image-swipe",
  "version": "2.0.0",
  "description": "A widget to easily swipe and zoom through images",
  "main": "image-swipe",
  "typings": "image-swipe.d.ts",
  "nativescript": {
    "platforms": {
      "ios": "3.0.0",
      "android": "3.0.0"
    },
    "plugin": {
        "nan": "true",
        "pan": "true",
        "core3": "true",
        "category": "Interface"
      }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PeterStaev/nativescript-image-swipe"
  },

  "keywords": [
    "NativeScript",
    "zoom",
    "whitebox",
    "swipe",
    "photo",
    "image",
    "tangra"
  ],
  "author": "Peter Staev <peter@tangrasoft.com>",
  "license": "Apache-2.0",

  "peerDependencies": {
     "tns-core-modules": "^3.0.0"
  },

  "devDependencies": {
    "typescript": "~2.2.2",
    "tslint": "5.2.0",
    "tns-core-modules": "3.0.1",
    "tns-platform-declarations": "3.0.1",

    "grunt": "1.0.1",
    "grunt-contrib-copy": "1.0.0",
    "grunt-contrib-clean": "1.0.0",
    "grunt-exec": "2.0.0"
  }

}