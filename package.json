{
  "name": "nativescript-image-swipe",
  "version": "3.0.0",
  "description": "A widget to easily swipe and zoom through images",
  "main": "image-swipe",
  "typings": "image-swipe.d.ts",
  "nativescript": {
    "platforms": {
      "ios": "4.0.0",
      "android": "4.0.0"
    },
    "plugin": {
      "nan": "true",
      "pan": "true",
      "core3": "true",
      "category": "Interface"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PeterStaev/nativescript-image-swipe"
  },
  "scripts": {
    "tslint": "tslint --project tsconfig.json",
    "tsc": "tsc --project tsconfig.json",
    "demo-ios": "grunt build && cd demo && tns run ios",
    "demo-android": "grunt build && cd demo && tns run android",
    "demo-ng-ios": "grunt build && cd demo-ng && tns run ios",
    "demo-ng-android": "grunt build && cd demo-ng && tns run android"
  },
  "keywords": [
    "NativeScript",
    "zoom",
    "whitebox",
    "swipe",
    "photo",
    "image",
    "tangra"
  ],
  "author": "Peter Staev <peter@tangrasoft.com>",
  "contributors": [
    "Dirk Rudolph (https://github.com/Buuhuu)"
  ],
  "license": "Apache-2.0",
  "peerDependencies": {
    "tns-core-modules": "^4"
  },
  "devDependencies": {
    "grunt": "1.0.1",
    "grunt-contrib-clean": "1.0.0",
    "grunt-contrib-copy": "1.0.0",
    "grunt-exec": "2.0.0",
    "tns-core-modules": "^4.0.1",
    "tns-platform-declarations": "^4.0.0",
    "tslint": "5.2.0",
    "typescript": "^2.6.2"
  }
}
